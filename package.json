{
  "name": "rxdb-simple-chat-app",
  "version": "1.0.0",
  "description": "simple chat application using RxDB",
  "main": "src/electron-main.js",
  "scripts": {
    "bundle": "rimraf dist && webpack",
    "watch": "rimraf dist && webpack -w",
    "http": "http-server -p 4000",
    "open:web": "wait-on dist/main.bundle.js && opener http://localhost:4000",
    "open:electron": "wait-on dist/main.bundle.js && electron .",
    "serve:db": "pouchdb-server -d ./database -p 5000",
    "clean:db": "rimraf database config.json",
    "start": "run-p bundle http open:web open:electron"
  },
  "keywords": ["rx", "rxdb", "electron", "pouchdb"],
  "author": "Quramy",
  "license": "MIT",
  "dependencies": {
    "abstract-leveldown": "^2.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "electron": "^1.4.10",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "material-ui": "^0.17.1",
    "memdown": "^1.2.4",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "opener": "^1.4.2",
    "pouchdb": "^6.0.7",
    "pouchdb-adapter-http": "^6.0.7",
    "pouchdb-adapter-leveldb": "^6.0.7",
    "pouchdb-adapter-memory": "^6.0.7",
    "pouchdb-replication": "^6.0.7",
    "pouchdb-server": "^2.3.6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-tap-event-plugin": "^2.0.1",
    "rimraf": "^2.5.4",
    "rxdb": "^3.0.6",
    "rxdb-adapter-localstorage": "^1.0.0",
    "rxjs": "^5.0.0-rc.4",
    "wait-on": "^2.0.1",
    "webpack": "beta"
  }
}
